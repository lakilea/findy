{
  "expo": {
    "name": "findy",
    "slug": "findy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash/mdpi/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#f69833"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.croone.findy"
    },
    "android": {
      "package": "com.croone.findy",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "splash": {
        "mdpi": "./assets/splash/mdpi/splash.png",
        "hdpi": "./assets/splash/hdpi/splash.png",
        "xhdpi": "./assets/splash/xhdpi/splash.png",
        "xxhdpi": "./assets/splash/xxhdpi/splash.png",
        "xxxhdpi": "./assets/splash/xxxhdpi/splash.png",
        "resizeMode": "cover",
        "backgroundColor": "#f69833"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
